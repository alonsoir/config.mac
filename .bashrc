# Minimal resource file just to be able to launch fish
# I keep bash as the default shell to make Emacs happy :-)
export TERM=xterm-256color
export PATH=${PATH}\
:/bin\
:/usr/bin\
:/usr/local/bin\
:/opt/local/bin\
:/Users/sylvain/.i3ci/bin

# Set keyboard repeat delay and rate
xset r rate 250 40

alias f='fish'

cd ~
